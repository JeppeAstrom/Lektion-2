@model SpecialOffer

<div class="offer-grid container-fluid">
    <img id="office-style" src="./@Model.Image1" alt="" />

    <div class="offer">
        <div class="red-border">
            <div class="content">
                <h1>@Model.Offer</h1>
                <p>@Model.Title</p>
                <button class="btn-white">FLASH SALE</button>
            </div>
        </div>
    </div>
    <img id="party-style" src="./@Model.Image2" alt="" />
</div>